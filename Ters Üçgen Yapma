public class TersUcgen {
    public static void main(String[] args) {
        int rows = 5; // Ters üçgenin satır sayısını belirleyin

        for (int i = rows; i >= 1; i--) {
            for (int j = 1; j <= i; j++) {
                System.out.print("* ");
            }
            System.out.println();
        }
    }
}
